    function LeftClick(X, Y, Times, Delay, Hold)
        if Delay == nil then
            Delay = 0
        end
        if Times == nil then
            Times = 1
        end
        if X == nil then
            X = 100
        end
        if Y == nil then
            Y = 100
        end
        for x = 1,Times do
            wait(Delay)
            
            vim:SendMouseButtonEvent(X, Y, 0, true, game, 1)
            if Hold ~= nil then
                wait(Hold)
            end
            vim:SendMouseButtonEvent(X, Y, 0, false, game, 1)
        end
    end
